# Example environment variables for Bharathi Enterprises app
# Copy this file to .env and set your own values

# Flask secret key - generate a random secret key for production
# You can generate one with: python -c "import os; print(os.urandom(24).hex())"
FLASK_SECRET_KEY=your-super-secret-key-here-change-this-in-production

# Database configuration (for future expansion)
DATABASE_URL=sqlite:///customers.db

# Security settings
BCRYPT_LOG_ROUNDS=12